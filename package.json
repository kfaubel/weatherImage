{
  "name": "weatherimage",
  "version": "1.0.0",
  "description": "Gets an image based on NWS weather data",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "build": "npm run tsc",
    "start": "npm run build && ts-node src/test.ts",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/test.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "prepare": "npm run build",
    "lint": "tslint -p tsconfig.json",
    "webpack": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kfaubel/weatherImage.git"
  },
  "author": "Ken Faubel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kfaubel/weatherImage/issues"
  },
  "homepage": "https://github.com/kfaubel/weatherImage#readme",
  "dependencies": {
    "@types/express": "^4.17.12",
    "axios": "^0.21.1",
    "express": "^4.17.3",
    "jpeg-js": "^0.4.3",
    "pureimage": "^0.3.2",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.4",
    "winston": "^3.3.3",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "prettier": "^2.3.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2"
  },
  "files": [
    "build/**/*",
    "fonts/**/*"
  ]
}
