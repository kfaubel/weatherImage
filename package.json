{
  "name": "weatherimage",
  "version": "1.0.0",
  "description": "Gets an image based on NWS weather data",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "build": "npm run tsc",
    "start": "ts-node src/app.ts",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/app.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "prepare": "npm run build",
    "lint": "tslint -p tsconfig.json",
    "webpack": "webpack",
    "devxxx": "npm run tsc && webpack --mode development ./build/weatherimage.js --output ./dist/main.js",
    "buildxxx": "npm run tsc && webpack --mode production ./build/weatherimage.js --output ./dist/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kfaubel/weatherImage.git"
  },
  "author": "Ken Faubel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kfaubel/weatherImage/issues"
  },
  "homepage": "https://github.com/kfaubel/weatherImage#readme",
  "dependencies": {
    "@types/express": "^4.17.0",
    "axios": "^0.19.0",
    "canvas": "^2.5.0",
    "express": "^4.17.1",
    "suncalc": "^1.8.0",
    "ts-node-dev": "^1.0.0-pre.40",
    "typescript": "^3.5.2",
    "winston": "^3.2.1",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "prettier": "^1.18.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7"
  },
  "files": [
    "build/**/*"
  ]
}
